<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">



    <changeSet id="20211230090223-1" author="jhipster">
        <createTable tableName="person_attribute">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="idattribute" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="attribute" type="varchar(255)">
                <constraints nullable="true" />
            </column>
        </createTable>
    </changeSet>

    <changeSet id="20211230090223-1-relations" author="jhipster">

    </changeSet>

    <changeSet id="20211230090223-1-data" author="jhipster" context="faker">
        <loadData
                file="db/changelog/liquibase/person_attribute.csv"
                separator=";"
                tableName="person_attribute">
            <column name="id" type="numeric"/>
            <column name="idattribute" type="numeric"/>
            <column name="attribute" type="string"/>
        </loadData>
    </changeSet>

</databaseChangeLog>